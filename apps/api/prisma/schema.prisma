generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Entry {
  id        String   @id @default(uuid())
  userId    String
  title     String
  note      String?
  date      DateTime?
  done      Boolean  @default(false)
  createdAt DateTime @default(now())
}